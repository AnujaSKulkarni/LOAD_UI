      <con:testStep type="groovy" name="Flow Control" id="022a5e00-210b-4831-9f69-1ab8f90201a7">
        <con:settings/>
        <con:config>
          <script>def dataCombination = context.expand( '${DataSource#Data_Combination}' )
def API_Name = context.expand( '${DataSource#API_Name}' )
def TestCaseName = testRunner.testCase.getName()

if (API_Name != TestCaseName){
	testRunner.testCase.testSteps['free vend_Positive'].setDisabled(true)
	testRunner.testCase.testSteps['free vend_Negative'].setDisabled(true)
	//testRunner.testCase.testSteps['Report'].setDisabled(true)
}else{
	if(dataCombination == "Positive"){
		testRunner.testCase.testSteps['free vend_Positive'].setDisabled(false)
		testRunner.testCase.testSteps['free vend_Negative'].setDisabled(true)
	//	testRunner.testCase.testSteps['Report'].setDisabled(false)
	}
	if(dataCombination == "Negative"){
		testRunner.testCase.testSteps['free vend_Positive'].setDisabled(true)
		testRunner.testCase.testSteps['free vend_Negative'].setDisabled(false)
	//	testRunner.testCase.testSteps['Report'].setDisabled(false)
	}
}</script>
